{
  "name": "@your-org/your-saas-platform",
  "version": "1.0.1",
  "private": false,
  "description": "Enterprise-ready SaaS admin dashboard with CRM, multi-tenancy, Stripe billing, and role-based access control",
  "keywords": [
    "saas",
    "admin-dashboard",
    "nextjs",
    "tailwind",
    "supabase",
    "crm",
    "multi-tenant",
    "stripe",
    "rbac",
    "template",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-info-network/tinadmin-saas-base-turborepo.git"
  },
  "bugs": {
    "url": "https://github.com/the-info-network/tinadmin-saas-base-turborepo/issues"
  },
  "homepage": "https://github.com/the-info-network/tinadmin-saas-base-turborepo#readme",
  "author": "Your Organization",
  "license": "MIT",
  "main": "./src/app/page.tsx",
  "files": [
    "src",
    "public",
    "supabase/migrations",
    "tailwind.config.ts",
    "tsconfig.json",
    "next.config.ts",
    "postcss.config.mjs",
    "README.md",
    "README_LOCAL_SETUP.md",
    "README_STRIPE.md"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.6.1",
  "scripts": {
    "initialize": "node scripts/initialize-fork.js",
    "dev": "turbo run dev",
    "dev:admin": "turbo run dev --filter=@tinadmin/admin",
    "dev:portal": "turbo run dev --filter=@tinadmin/portal",
    "dev:wizards-api": "turbo run dev --filter=@magicwizards/wizards-api",
    "dev:local": "supabase start && pnpm dev",
    "dev:localhost": "./scripts/start-localhost.sh",
    "supabase:env:remote": "node scripts/install-remote-supabase-env.mjs jruxnkslobykshunucwa",
    "supabase:sync-vercel": "node scripts/sync-vercel-to-supabase-auth.mjs jruxnkslobykshunucwa",
    "supabase:install-gohighlevel": "node scripts/install-gohighlevel-provider.mjs",
    "supabase:apply-integration-migration": "node scripts/apply-integration-migration.mjs",
    "build": "next build",
    "build:packages": "turbo run build --filter='./packages/*'",
    "build:admin": "turbo run build --filter=@tinadmin/admin",
    "build:portal": "turbo run build --filter=@tinadmin/portal",
    "build:wizards-api": "turbo run build --filter=@magicwizards/wizards-api",
    "build:all": "turbo run build",
    "start": "turbo run start",
    "start:admin": "turbo run start --filter=@tinadmin/admin",
    "start:portal": "turbo run start --filter=@tinadmin/portal",
    "start:wizards-api": "turbo run start --filter=@magicwizards/wizards-api",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "type-check:wizards-api": "turbo run type-check --filter=@magicwizards/wizards-api",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "audit": "pnpm audit --audit-level=moderate",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "docker:ps": "docker ps | grep supabase",
    "docker:logs": "supabase logs"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@heroicons/react": "^2.2.0",
    "@popperjs/core": "^2.11.8",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.9",
    "apexcharts": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "flatpickr": "^4.6.13",
    "next": "^16.1.1",
    "prismjs": "^1.30.0",
    "react": "^19.2.3",
    "react-apexcharts": "^1.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.5",
    "server-only": "^0.0.1",
    "simplebar-react": "^3.3.0",
    "stripe": "^20.0.0",
    "swiper": "^11.2.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/prismjs": "^1.26.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "postcss": "^8",
    "turbo": "^2.7.1",
    "typescript": "5.9.3",
    "vitest": "^2.1.2"
  }
}
